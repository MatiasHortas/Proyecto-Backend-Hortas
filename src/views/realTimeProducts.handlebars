<h2>Agregar Tu producto:</h2>
<form id="form">
  <input
    type="text"
    name="tittle"
    id="tittle"
    placeholder="Nombre del producto"
    required
  />
  <input
    type="text"
    name="desc"
    id="desc"
    placeholder="Descripcion del producto"
    required
  />
  <input
    type="number"
    name="price"
    id="price"
    placeholder="Precio del producto"
    required
  />
  <input
    type="text"
    name="thumbnail"
    id="thumbnail"
    placeholder="Link del producto"
    required
  />
  <input
    type="number"
    name="stock"
    id="stock"
    placeholder="Stock del producto"
    required
  />
  <input
    type="text"
    name="code"
    id="code"
    placeholder="Codigo del producto"
    required
  />
  <input
    type="text"
    name="category"
    id="category"
    placeholder="Categoria del producto"
    required
  />

  <input type="submit" value="ENVIAR" />
</form>
<ul id="listaProducto">

  {{#each listaProducto}}
    <p id="titleP">Titulo: {{this.tittle}}</p>
    <p>Descripcion: {{this.desc}}</p>
    <p>Precio: {{this.price}}</p>
    <p>Link: {{this.thumbnail}}</p>
    <p>Stock: {{this.stock}}</p>
    <p>Codigo: {{this.code}}</p>
    <p>Categoria: {{this.category}}</p>
  {{/each}}

</ul>

<script src="/socket.io/socket.io.js"></script>;
<script src="/js/index.js"></script>